    },
    {
      "name": "Microsoft",
      "roles": [
        "host"
      ],
      "url": "https://planetarycomputer.microsoft.com/"
    }
  ],
  "summaries": {
    "deltares:reservoir": [
      "ERA5",
      "CHIRPS",
      "EOBS",
      "NLDAS",
      "BOM"
    ]
  },
  "assets": {
    "thumbnail": {
      "href": "https://ai4edatasetspublicassets.azureedge.net/assets/pc_thumbnails/additional_datasets/deltares-reservoir.jpg",
